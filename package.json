{
  "name": "reactive-form",
  "version": "1.0.0",
  "description": "A reactive basic form ready to submit made with react,redux and setup with webpack",
  "main": "index.js",
  "scripts": {
    "watch-css": "npm run build-css && node-sass -w ./src/styles/styles.scss ./src/styles/styles.css",
    "build-css": "node-sass ./src/styles/styles.scss ./src/styles/styles.css",
    "build-prod": "webpack --mode production",
    "build-dev": "webpack --mode development",
    "build-all": "npm-run-all build-server build-prod",
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "webpack-dev-server --config ./webpack.config.js --mode development",
    "test": "jest --env=jsdom --watch",
    "coverage": "jest --env=jsdom --coverage",
    "e2e": "./node_modules/webdriverio/bin/wdio",
    "build-server": "./scripts/build-server.sh",
    "fast-build-server": "./scripts/fast-build.sh",
    "server": "node server/dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ManuelPereira2016/reactive-form.git"
  },
  "author": "Manuel Pereira",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ManuelPereira2016/reactive-form/issues"
  },
  "homepage": "https://github.com/ManuelPereira2016/reactive-form#readme",
  "dependencies": {
    "await-to-js": "^2.0.1",
    "axios": "^0.18.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "font-awesome": "^4.7.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.1",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^16.4.1",
    "react-flexbox-grid": "^2.1.2",
    "react-hot-loader": "^4.3.3",
    "react-md-spinner": "^0.2.5",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-devtools": "^3.4.1",
    "redux-thunk": "^2.3.0",
    "regenerator": "^0.13.2",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "devDependencies": {
    "babel-jest": "^23.2.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^23.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "moment": "^2.22.2",
    "react-test-renderer": "^16.4.1",
    "redux-mock-store": "^1.5.3",
    "sinon": "^6.1.0",
    "wdio": "^2.0.2",
    "wdio-chromedriver-service": "^0.1.3",
    "wdio-jasmine-framework": "0.3.4",
    "wdio-selenium-standalone-service": "^0.0.10",
    "webdriverio": "^4.13.1"
  }
}
